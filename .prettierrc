{
  "singleQuote": true,
  "semi": false,
  "trailingComma": "none",
  "printWidth": 120,
  "arrowParens": "always",
  "tabWidth": 2,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.vue",
      "options": {
        "parser": "vue",
        "printWidth": 120
      }
    }
  ]
}
